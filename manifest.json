{
	"manifest_version": 2,
	
	"name": "Visualize This",
	"description": "HTML5 Audio Visualizer",
	"version": "1.0",
	
	"content_scripts": [ {
		"matches": [ "*://play.google.com/music/*", "*://www.youtube.com/watch*" ],
		"run_at": "document_end",
		"js": [ "js/jquery.min.js", "js/visualizer.js", "js/gpm-inject.js" ]
	} ],
	
	"icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
   },
   
   "web_accessible_resources": [
		"css/*.css",
		"images/*.png",
		"html/*.html"
	]
}